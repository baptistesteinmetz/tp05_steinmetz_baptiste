{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tp02": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "front/src",
      "sourceRoot": "/",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/tp02",
            "index": "index.html",
            "main": "main.ts",
            "polyfills": "polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "./front/src/favicon.ico",
              "./front/src/assets/"
            ],
            "styles": [
              "styles.scss"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "./front/src/environments/environment.ts",
                  "with": "./front/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tp02:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tp02:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tp02:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "./front/src/test.ts",
            "polyfills": "./front/src/polyfills.ts",
            "tsConfig": "./front/tsconfig.spec.json",
            "karmaConfig": "./front/karma.conf.js",
            "assets": [
              "./front/src/favicon.ico",
              "./front/src/assets"
            ],
            "styles": [
              "./front/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "./front/tsconfig.app.json",
              "./front/tsconfig.spec.json",
              "./e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "tp02:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tp02:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "tp02"
}
